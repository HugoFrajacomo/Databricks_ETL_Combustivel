    {
      "name": "b6790011",
      "displayName": "fact_fuel_last_12_months",
      "queryLines": [
        "WITH max_date AS (\r\n",
        "    SELECT MAX(DataColeta) AS dt_max\r\n",
        "    FROM gov_br_combustivel.silver_layer.fact_fuel\r\n",
        ")\r\n",
        "\r\n",
        "SELECT FF.*\r\n",
        "FROM gov_br_combustivel.silver_layer.fact_fuel AS FF\r\n",
        "CROSS JOIN max_date\r\n",
        "WHERE FF.DataColeta >= add_months(dt_max, -12)"
      ]
    },
